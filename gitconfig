[color]
  ui = auto
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green
[color "diff"]
  meta = yellow
  frag = magenta
  old = red
  new = green
[color "status"]
  added = yellow
  changed = green
  untracked = cyan
[color "diff"]
  whitespace = red reverse
[core]
  whitespace = fix,-indent-with-non-tab,trailing-space,cr-at-eol
  excludesfile = /Users/tadas/.gitignore_global
  editor = /usr/local/bin/vim
  warnambiguousrefs = false
  pager = `brew --prefix git`/share/git-core/contrib/diff-highlight/diff-highlight | less
[alias]
  co = checkout
  ci = commit -v
  cia = commit -av
  cp = cherry-pick
  st = status
  br = branch
  type = cat-file -t
  dump = cat-file -p
  df = diff
  dc = diff --cached
  lg = log --pretty=format:'%Cred%h%Creset -%Creset %s %Cgreen(%cr) %Creset- %Cblue%an%Creset'
  lol = log --graph --decorate --pretty=oneline --abbrev-commit
  lola = log --graph --decorate --pretty=oneline --abbrev-commit --all
  hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
  ls = ls-files
  ignored = ls-files -o -i --exclude-standard
  discard = checkout -- .
  rmmissing = "!ruby -e 'x = `git st`;files = x.split(\"\n\").select{|x|x=~/deleted/ }.map{|x|x.split(\" \")[2]}.join(\" \"); files.each{|f| `git rm #{f}`}'"
  praise = blame
  uncommit = reset --soft HEAD^
  startover = reset --hard HEAD
  ready = rebase -i @{u}
  g = grep --break --heading --line-number
